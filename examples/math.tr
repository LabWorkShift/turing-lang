fn add(a, b) {
    return a + b
}

fn subtract(a, b) {
    return a - b
}

fn multiply(a, b) {
    return a * b
}

fn divide(a, b) {
    if b == 0 {
        return 0
    }
    return a / b
}

fn power(base, exp) {
    if exp == 0 {
        return 1
    }
    
    let result = 1
    let counter = 0
    
    while counter < exp {
        result = result * base
        counter = counter + 1
    }
    
    return result
}
